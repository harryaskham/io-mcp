
# <djent-init>
# Added by djent init
path_remove() {
  echo ":$PATH:" | sed "s|:$1:|:|g" | sed 's/^://;s/:$//'
}

export SCRIPTS_PATH="$HOME/.config/djent/bin"
export OG_PATH=$(path_remove "$SCRIPTS_PATH")
export PATH="$SCRIPTS_PATH:$OG_PATH"

# Prepend .djent/bin so a worktree-local snapshot binary takes precedence
export PATH="$PWD/.djent/bin:$PATH"
# </djent-init>


export IO_MCP_URL=http://localhost:8444/mcp
